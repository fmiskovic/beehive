[loggers]
keys=root,tw

[handlers]
keys=consoleHandler

[formatters]
keys=twFormatter

[logger_root]
level=DEBUG
handlers=consoleHandler

[logger_tw]
level=DEBUG
handlers=consoleHandler
qualname=twLogger
propagate=1

[handler_consoleHandler]
class=StreamHandler
level=DEBUG
formatter=twFormatter
args=(sys.stdout,)

[formatter_twFormatter]
format=[%(asctime)s] [PID: %(process)d] [%(name)s] [%(levelname)s] [%(message)s]