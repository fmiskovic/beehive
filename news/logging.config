[loggers]
keys=root,news

[handlers]
keys=consoleHandler

[formatters]
keys=newsFormatter

[logger_root]
level=DEBUG
handlers=consoleHandler

[logger_news]
level=DEBUG
handlers=consoleHandler
qualname=newsLogger
propagate=1

[handler_consoleHandler]
class=StreamHandler
level=DEBUG
formatter=newsFormatter
args=(sys.stdout,)

[formatter_newsFormatter]
format=[%(asctime)s] [PID: %(process)d] [%(name)s] [%(levelname)s] [%(message)s]