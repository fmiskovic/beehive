[loggers]
keys=root,news

[handlers]
keys=consoleHandler

[formatters]
keys=botFormatter

[logger_root]
level=DEBUG
handlers=consoleHandler

[logger_news]
level=DEBUG
handlers=consoleHandler
qualname=botLogger
propagate=0

[handler_consoleHandler]
class=StreamHandler
level=DEBUG
formatter=botFormatter
args=(sys.stdout,)

[formatter_botFormatter]
format=[%(asctime)s] [PID: %(process)d] [%(name)s] [%(levelname)s] [%(message)s]